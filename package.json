{
  "name": "github-links",
  "displayName": "Github Links",
  "description": "This extension opens up the github links in your browser",
  "version": "1.0.0",
  "publisher": "malachiirwin",
  "engines": {
    "vscode": "^1.37.0"
  },
  "repository": {
    "url": "https://github.com/malachirwin/github-links"
  },
  "categories": [
    "SCM Providers"
  ],
  "icon": "logo.jpeg",
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.github_history",
        "title": "Open File GitHub History"
      },
      {
        "command": "extension.github_page",
        "title": "Open File GitHub Page"
      },
      {
        "command": "extension.github_blame",
        "title": "Open File GitHub Blame url"
      },
      {
        "command": "extension.open_file_clipboard",
        "title": "Open File From GitHub url in clipboard"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "webpack --mode development",
    "watch": "webpack --watch --mode development",
    "pretest": "npm run compile",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/git-branch": "^2.0.1",
    "@types/glob": "^7.1.1",
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.137",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/voca": "^1.4.0",
    "@types/vscode": "^1.37.0",
    "babel-jest": "^29.7.0",
    "glob": "^7.1.4",
    "jest": "^29.7.0",
    "mocha": "^6.1.4",
    "ts-loader": "^9.2.5",
    "tslint": "^5.12.1",
    "typescript": "^5.2.2",
    "vscode-test": "^1.0.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "git-branch": "^2.0.1",
    "git-remote-origin-url": "^3.0.0",
    "lodash": "^4.17.15",
    "simple-git": "^1.124.0",
    "voca": "^1.4.0"
  }
}
